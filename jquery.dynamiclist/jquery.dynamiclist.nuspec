<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>jQuery.Dynamiclist</id>
    <version>1.0</version>
    <authors>John Rummell</authors>
    <owners>John Rummell</owners>
    <projectUrl>https://github.com/jrummell/jquery.dynamiclist/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A jQuery plugin for MVC that supports adding and removing items from a list Model.</description>
    <tags>jquery plugin list</tags>
    <dependencies>
      <dependency id="Microsoft.jQuery.Unobtrusive.Validation" version="3.4.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="Scripts\Package\jquery.dynamiclist*.js" target="content\Scripts"  exclude="Scripts\Package\jquery.dynamiclist*templates*.js"/>
    <file src="Scripts\Package\jquery.validate.unobtrusive.dynamic*.js" target="content\Scripts"/>
  </files>
</package>
